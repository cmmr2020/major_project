<view class="container">
  <scroll-view scroll-y class="scrollPage">
    <view class="tui-article-pic">
      <image src='../../../../images/haibao.png' mode='widthFix' class='png' style='width:100%;height:486rpx'></image>
    </view>
    <view class='nav-list' wx:if="{{projectType=='0'}}">
      <navigator open-type="navigate" hover-class='none' url="../allInfo/allInfo?projectId={{item.id}}&projectName={{item.name}}&govId={{govId}}" class="nav-li bg-{{item.color}}" wx:for="{{elements}}" wx:key="index"> 
        <view class="nav-title">{{item.name}}</view>
        <view class="nav-name">Project</view>
      </navigator>
    </view>
    <view class='nav-list' wx:if="{{projectType=='1'}}">
      <navigator open-type="navigate" hover-class='none' url="../map/map?projectId={{item.id}}&projectName={{item.name}}" class="nav-li bg-{{item.color}}" wx:for="{{elements}}" wx:key="index"> 
        <view class="nav-title">{{item.name}}</view>
        <view class="nav-name">Project</view>
      </navigator>
    </view>
    <view class='cu-tabbar-height'></view>
  </scroll-view>
</view>